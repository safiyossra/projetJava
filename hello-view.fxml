<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="589.0" prefWidth="488.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ma.fstt.trackingl.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="     Tableau Livreur" wrappingWidth="351.9830722808838" />
      <Label text="Nom de livreur :" />
      <TextField fx:id="nom" />
      <Label text="Telephone" />
      <TextField fx:id="tele" />
      <Button fx:id="ok" minHeight="15.0" mnemonicParsing="false" onAction="#onSaveButtonClick" prefHeight="55.0" prefWidth="94.0" text="Save" />
       <Button fx:id="oui" mnemonicParsing="false" onAction="#onUpdateButtonClick" prefHeight="51.0" prefWidth="96.0" text="Update" />
       <Button fx:id="yes" minHeight="21.0" mnemonicParsing="false" onAction="#ondeleteButtonClick" prefHeight="50.0" prefWidth="94.0" text="Delete" />
      <TableView fx:id="mytable" prefHeight="138.0" onMouseClicked="#onRowClick" prefWidth="448.0">
        <columns>
          <TableColumn fx:id="col_id" prefWidth="75.0" text="id" />
          <TableColumn fx:id="col_nom" prefWidth="75.0" text="nom" />
            <TableColumn fx:id="col_tele" prefWidth="75.0" text="telephone" />

        </columns>
      </TableView>

    </children>



</VBox>
